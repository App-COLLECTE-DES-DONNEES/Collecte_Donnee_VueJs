A,KAAK,CAACG,UAAV;AAAA,GAFN;AAGZ4B,EAAAA,UAAU,EAAE,oBAAA/B,KAAK;AAAA,WAAIA,KAAK,CAACK,IAAV;AAAA,GAHL;AAIZ2B,EAAAA,UAAU,EAAE,oBAAAhC,KAAK;AAAA,WAAIA,KAAK,CAACI,SAAV;AAAA,GAJL;AAKZ6B,EAAAA,SAAS,EAAE,mBAAAjC,KAAK;AAAA,WAAIA,KAAK,CAACM,SAAV;AAAA,GALJ;AAMZ4B,EAAAA,cAAc,EAAE,wBAAAlC,KAAK;AAAA,WAAIA,KAAK,CAACQ,WAAV;AAAA;AANT,CAAhB;AAQA,IAAM2B,SAAS,GAAG;AACdC,EAAAA,YAAY,EAAE,sBAACpC,KAAD,EAAOE,WAAP;AAAA,WAAqBF,KAAK,CAACE,WAAN,GAAkBA,WAAvC;AAAA,GADA;AAEdmC,EAAAA,WAAW,EAAE,qBAACrC,KAAD,EAAOG,UAAP;AAAA,WAAoBH,KAAK,CAACG,UAAN,GAAiBA,UAArC;AAAA,GAFC;AAGdmC,EAAAA,UAAU,EAAE,oBAACtC,KAAD,EAAOI,SAAP;AAAA,WAAmBJ,KAAK,CAACI,SAAN,GAAgBA,SAAnC;AAAA,GAHE;AAIdmC,EAAAA,UAAU,EAAE,oBAACvC,KAAD,EAAOK,IAAP;AAAA,WAAcL,KAAK,CAACK,IAAN,GAAWA,IAAzB;AAAA,GAJE;AAKdmC,EAAAA,SAAS,EAAE,mBAACxC,KAAD,EAAOyC,MAAP;AAAA,WAAgBzC,KAAK,CAACM,SAAN,GAAgBmC,MAAhC;AAAA,GALG;AAMdC,EAAAA,cAAc,EAAE,wBAAC1C,KAAD,EAAOyC,MAAP;AAAA,WAAgBzC,KAAK,CAACQ,WAAN,GAAkBiC,MAAlC;AAAA;AANF,CAAlB;AASA,eAAe;AACXzC,EAAAA,KAAK,EAALA,KADW;AACL4B,EAAAA,OAAO,EAAPA,OADK;AACGnB,EAAAA,OAAO,EAAPA,OADH;AACW0B,EAAAA,SAAS,EAATA;AADX,CAAf","sourcesContent":["import axios from 'axios'\r\nimport constants from '../../plugins/constants'\r\n\r\nconst state = {\r\n    resource_url: constants.resource_url,\r\n    list_kanban:[],\r\n    list_graph:[],\r\n    list_hist:[],\r\n    list:[],\r\n    statuscat:false,\r\n    colors:[\"#003473\",\"#EB9F07\"],\r\n    catproducts:[]\r\n\r\n}\r\n\r\nconst actions = {\r\n\r\n    async FetchVueKanban({commit},value){\r\n\r\n        axios.get(state.resource_url+value+'/kanbanview')\r\n            .then(list => {\r\n                if(list.data.success){\r\n                    commit(\"SETVUEKANBAN\",list.data.data)\r\n                }\r\n\r\n            })\r\n            .catch(function(error) {\r\n                console.log('products_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n                // var errorMessage = error.message;\r\n                // console.log(error);\r\n\r\n                //commit(\"setError\", error);\r\n\r\n            });\r\n\r\n    },\r\n\r\n    async FetchVueGraph({commit},value){\r\n\r\n        axios.get(state.resource_url+value+'/graphview')\r\n            .then(list => {\r\n                if(list.data.success){\r\n                    commit(\"SETVUEGRAPH\",list.data.data)\r\n                    commit(\"SETVUEHIST\",list.data.datas)\r\n                }\r\n\r\n            })\r\n            .catch(function(error) {\r\n                console.log('products_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n                // var errorMessage = error.message;\r\n                // console.log(error);\r\n\r\n                //commit(\"setError\", error);\r\n\r\n            });\r\n\r\n    },\r\n\r\n    async FetchVueList({commit},value){\r\n\r\n        axios.get(state.resource_url+value+'/listview')\r\n\r\n            .then(list => {\r\n\r\n                if(list.data.success){\r\n\r\n                    commit(\"SETVUELIST\",list.data.data)\r\n\r\n                }\r\n\r\n            })\r\n            .catch(function(error) {\r\n                console.log('products_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n                // var errorMessage = error.message;\r\n                // console.log(error);\r\n\r\n                //commit(\"setError\", error);\r\n\r\n            });\r\n\r\n    },\r\n\r\n    async registercatProduct({commit},data){\r\n\r\n        axios.post(state.resource_url+data.variant,data)\r\n\r\n            .then(customers => {\r\n\r\n                if(customers.data.success) {\r\n                    //objet client qui vient detre ajoute\r\n                    commit(\"SETCATPRODUCTS\", customers.data.data)\r\n                    commit(\"SETSTATUS\", true)\r\n                }\r\n            })\r\n            .catch(function(error) {\r\n                console.log('customer_create_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n\r\n            });\r\n\r\n    }\r\n\r\n}\r\n\r\nconst getters = {\r\n    GetVueKanban: state => state.list_kanban,\r\n    GetVueGraph: state => state.list_graph,\r\n    GetVueList: state => state.list,\r\n    GetVueHist: state => state.list_hist,\r\n    GetStatus: state => state.statuscat,\r\n    GetCatProducts: state => state.catproducts,\r\n}\r\nconst mutations = {\r\n    SETVUEKANBAN: (state,list_kanban)=>state.list_kanban=list_kanban,\r\n    SETVUEGRAPH: (state,list_graph)=>state.list_graph=list_graph,\r\n    SETVUEHIST: (state,list_hist)=>state.list_hist=list_hist,\r\n    SETVUELIST: (state,list)=>state.list=list,\r\n    SETSTATUS: (state,status)=>state.statuscat=status,\r\n    SETCATPRODUCTS: (state,status)=>state.catproducts=status,\r\n}\r\n\r\nexport default {\r\n    state,getters,actions,mutations\r\n}\r\n"]}]}