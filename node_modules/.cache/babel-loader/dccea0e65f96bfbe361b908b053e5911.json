CC,GAAR,CAAY,uBAAZ,EAAoCY,UAAU,CAACjB,IAAX,CAAgBC,OAApD,EADgB,CAGjB;AACK;;AACAJ,gBAAAA,MAAM,CAAC,eAAD,EAAkBoB,UAAU,CAACjB,IAAX,CAAgBC,OAAlC,CAAN,CALY,CAOhB;AACH,eAVL,EAYKC,KAZL,CAYW,UAASC,KAAT,EAAgB;AACnBC,gBAAAA,OAAO,CAACC,GAAR,CAAY,uBAAZ,EAAoCF,KAApC,EADmB,CAEnB;AACA;AAEH,eAjBL;;AAJgC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuBnC;AAvLW,CAAhB;AA2LA,IAAMe,OAAO,GAAG;AACZC,EAAAA,YAAY,EAAE,sBAAArC,KAAK;AAAA,WAAIA,KAAK,CAACE,WAAV;AAAA,GADP;AAEZoC,EAAAA,WAAW,EAAE,qBAAAtC,KAAK;AAAA,WAAIA,KAAK,CAACG,UAAV;AAAA,GAFN;AAGZoC,EAAAA,UAAU,EAAE,oBAAAvC,KAAK;AAAA,WAAIA,KAAK,CAACK,IAAV;AAAA,GAHL;AAIZmC,EAAAA,UAAU,EAAE,oBAAAxC,KAAK;AAAA,WAAIA,KAAK,CAACI,SAAV;AAAA,GAJL;AAKZqC,EAAAA,SAAS,EAAE,mBAAAzC,KAAK;AAAA,WAAIA,KAAK,CAACM,SAAV;AAAA,GALJ;AAMZoC,EAAAA,aAAa,EAAE,uBAAA1C,KAAK;AAAA,WAAIA,KAAK,CAACU,UAAV;AAAA,GANR;AAOZiC,EAAAA,cAAc,EAAE,wBAAA3C,KAAK;AAAA,WAAIA,KAAK,CAACQ,WAAV;AAAA,GAPT;AAQZoC,EAAAA,WAAW,EAAE,qBAAA5C,KAAK;AAAA,WAAIA,KAAK,CAACS,QAAV;AAAA,GARN;AASZoC,EAAAA,gBAAgB,EAAE,0BAAA7C,KAAK;AAAA,WAAIA,KAAK,CAACW,aAAV;AAAA;AATX,CAAhB;AAWA,IAAMmC,SAAS,GAAG;AACdC,EAAAA,YAAY,EAAE,sBAAC/C,KAAD,EAAOE,WAAP;AAAA,WAAqBF,KAAK,CAACE,WAAN,GAAkBA,WAAvC;AAAA,GADA;AAEd8C,EAAAA,WAAW,EAAE,qBAAChD,KAAD,EAAOG,UAAP;AAAA,WAAoBH,KAAK,CAACG,UAAN,GAAiBA,UAArC;AAAA,GAFC;AAGd8C,EAAAA,UAAU,EAAE,oBAACjD,KAAD,EAAOI,SAAP;AAAA,WAAmBJ,KAAK,CAACI,SAAN,GAAgBA,SAAnC;AAAA,GAHE;AAId8C,EAAAA,UAAU,EAAE,oBAAClD,KAAD,EAAOK,IAAP;AAAA,WAAcL,KAAK,CAACK,IAAN,GAAWA,IAAzB;AAAA,GAJE;AAKd8C,EAAAA,SAAS,EAAE,mBAACnD,KAAD,EAAOoD,MAAP;AAAA,WAAgBpD,KAAK,CAACM,SAAN,GAAgB8C,MAAhC;AAAA,GALG;AAMdC,EAAAA,cAAc,EAAE,wBAACrD,KAAD,EAAOoD,MAAP;AAAA,WAAgBpD,KAAK,CAACQ,WAAN,GAAkB4C,MAAlC;AAAA,GANF;AAOdE,EAAAA,WAAW,EAAE,qBAACtD,KAAD,EAAOoD,MAAP;AAAA,WAAgBpD,KAAK,CAACS,QAAN,GAAe2C,MAA/B;AAAA,GAPC;AAQdG,EAAAA,aAAa,EAAE,uBAACvD,KAAD,EAAOoD,MAAP;AAAA,WAAgBpD,KAAK,CAACU,UAAN,GAAiB0C,MAAjC;AAAA,GARD;AASdI,EAAAA,gBAAgB,EAAE,0BAACxD,KAAD,EAAOoD,MAAP;AAAA,WAAgBpD,KAAK,CAACW,aAAN,GAAoByC,MAApC;AAAA;AATJ,CAAlB;AAYA,eAAe;AACXpD,EAAAA,KAAK,EAALA,KADW;AACLoC,EAAAA,OAAO,EAAPA,OADK;AACGxB,EAAAA,OAAO,EAAPA,OADH;AACWkC,EAAAA,SAAS,EAATA;AADX,CAAf","sourcesContent":["import axios from 'axios'\r\nimport constants from '../../plugins/constants'\r\n\r\nconst state = {\r\n    resource_url: constants.resource_url,\r\n    list_kanban:[],\r\n    list_graph:[],\r\n    list_hist:[],\r\n    list:[],\r\n    statuscat:false,\r\n    colors:[\"#003473\",\"#EB9F07\"],\r\n    catproducts:[],\r\n    products:[],\r\n    statuspost:false,\r\n    statusapprove:false\r\n}\r\n\r\nconst actions = {\r\n\r\n    async FetchVueKanban({commit},value){\r\n\r\n        axios.get(state.resource_url+value+'/kanbanview')\r\n            .then(list => {\r\n                if(list.data.success){\r\n                    commit(\"SETVUEKANBAN\",list.data.data)\r\n                }\r\n\r\n            })\r\n            .catch(function(error) {\r\n                console.log('products_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n                // var errorMessage = error.message;\r\n                // console.log(error);\r\n\r\n                //commit(\"setError\", error);\r\n\r\n            });\r\n\r\n    },\r\n\r\n    async FetchVueGraph({commit},value){\r\n\r\n        axios.get(state.resource_url+value+'/graphview')\r\n            .then(list => {\r\n                if(list.data.success){\r\n                    commit(\"SETVUEGRAPH\",list.data.data)\r\n                    commit(\"SETVUEHIST\",list.data.datas)\r\n                }\r\n\r\n            })\r\n            .catch(function(error) {\r\n                console.log('products_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n                // var errorMessage = error.message;\r\n                // console.log(error);\r\n\r\n                //commit(\"setError\", error);\r\n\r\n            });\r\n\r\n    },\r\n\r\n    async FetchVueList({commit},value){\r\n\r\n        axios.get(state.resource_url+value+'/listview')\r\n\r\n            .then(list => {\r\n\r\n                if(list.data.success){\r\n\r\n                    commit(\"SETVUELIST\",list.data.data)\r\n\r\n                }\r\n\r\n            })\r\n            .catch(function(error) {\r\n                console.log('products_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n                // var errorMessage = error.message;\r\n                // console.log(error);\r\n\r\n                //commit(\"setError\", error);\r\n\r\n            });\r\n\r\n    },\r\n\r\n    async FetchVueVisitList({commit},value){\r\n\r\n        axios.get(state.resource_url+'visite/listview/'+value)\r\n\r\n            .then(list => {\r\n\r\n                if(list.data.success){\r\n\r\n                    commit(\"SETVUELIST\",list.data.data)\r\n\r\n                }\r\n\r\n            })\r\n            .catch(function(error) {\r\n                console.log('products_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n                // var errorMessage = error.message;\r\n                // console.log(error);\r\n\r\n                //commit(\"setError\", error);\r\n\r\n            });\r\n\r\n    },\r\n\r\n\r\n    async registercatProduct({commit},data){\r\n\r\n        axios.post(state.resource_url+data.variant,data)\r\n\r\n            .then(customers => {\r\n\r\n                if(customers.data.success) {\r\n                    //objet client qui vient detre ajoute\r\n                    commit(\"SETCATPRODUCTS\", customers.data.data)\r\n                    commit(\"SETSTATUS\", true)\r\n                }\r\n            })\r\n            .catch(function(error) {\r\n                console.log('customer_create_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n\r\n            });\r\n\r\n    },\r\n\r\n    async approve({commit},data){\r\n\r\n        axios.get(state.resource_url+'visites/approuver/'+data)\r\n\r\n            .then(approve => {\r\n\r\n                if(approve.data.success) {\r\n                    //objet client qui vient detre ajoute\r\n                    commit(\"SETSTATUSAPPROVE\", true)\r\n                }\r\n            })\r\n            .catch(function(error) {\r\n                console.log('customer_create_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n\r\n            });\r\n\r\n    },\r\n\r\n    async registerproduct({commit},data){\r\n\r\n        axios.post(state.resource_url+data.get('variant'),data)\r\n\r\n            .then(customers => {\r\n\r\n                if(customers.data.success) {\r\n                    //objet client qui vient detre ajoute\r\n                    commit(\"SETPRODUCTS\", customers.data.data)\r\n                }\r\n            })\r\n            .catch(function(error) {\r\n                console.log('customer_create_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n\r\n            });\r\n\r\n    },\r\n    async startinspection({commit},data){\r\n\r\n        console.log('data',data)\r\n\r\n        axios.post(state.resource_url+\"inspections\",data)\r\n\r\n            .then(inspection => {\r\n                console.log('customer_create_error',inspection.data.success);\r\n\r\n               // if(inspection.data.success) {\r\n                    //objet client qui vient detre ajoute\r\n                    commit(\"SETSTATUSPOST\", inspection.data.success)\r\n\r\n                //}\r\n            })\r\n\r\n            .catch(function(error) {\r\n                console.log('customer_create_error',error);\r\n                // Handle Errors here.\r\n                // var errorCode = error.code;\r\n\r\n            });\r\n\r\n    }\r\n\r\n}\r\n\r\nconst getters = {\r\n    GetVueKanban: state => state.list_kanban,\r\n    GetVueGraph: state => state.list_graph,\r\n    GetVueList: state => state.list,\r\n    GetVueHist: state => state.list_hist,\r\n    GetStatus: state => state.statuscat,\r\n    GetStatusPost: state => state.statuspost,\r\n    GetCatProducts: state => state.catproducts,\r\n    GetProducts: state => state.products,\r\n    getStatusApprove: state => state.statusapprove,\r\n}\r\nconst mutations = {\r\n    SETVUEKANBAN: (state,list_kanban)=>state.list_kanban=list_kanban,\r\n    SETVUEGRAPH: (state,list_graph)=>state.list_graph=list_graph,\r\n    SETVUEHIST: (state,list_hist)=>state.list_hist=list_hist,\r\n    SETVUELIST: (state,list)=>state.list=list,\r\n    SETSTATUS: (state,status)=>state.statuscat=status,\r\n    SETCATPRODUCTS: (state,status)=>state.catproducts=status,\r\n    SETPRODUCTS: (state,status)=>state.products=status,\r\n    SETSTATUSPOST: (state,status)=>state.statuspost=status,\r\n    SETSTATUSAPPROVE: (state,status)=>state.statusapprove=status,\r\n}\r\n\r\nexport default {\r\n    state,getters,actions,mutations\r\n}\r\n"]}]}