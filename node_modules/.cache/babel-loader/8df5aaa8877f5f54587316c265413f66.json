ACA;AACA;AACA;AACA;AACH,eApBL;;AAFwB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuB3B;AAzKW,CAAhB;AA4KA,IAAMY,OAAO,GAAG;AACZC,EAAAA,WAAW,EAAE,qBAAApC,KAAK;AAAA,WAAIA,KAAK,CAACE,QAAV;AAAA,GADN;AAEZmC,EAAAA,cAAc,EAAE,wBAAArC,KAAK;AAAA,WAAIA,KAAK,CAACC,WAAV;AAAA,GAFT;AAGZ;AACAqC,EAAAA,MAAM,EAAC,gBAAAtC,KAAK;AAAA,WAAIA,KAAK,CAACO,GAAV;AAAA,GAJA;AAKZgC,EAAAA,gBAAgB,EAAC,0BAAAvC,KAAK;AAAA,WAAIA,KAAK,CAACQ,aAAV;AAAA,GALV;AAMZgC,EAAAA,WAAW,EAAC,qBAAAxC,KAAK;AAAA,WAAIA,KAAK,CAACU,SAAV;AAAA;AANL,CAAhB;AASA,IAAM+B,SAAS,GAAG;AAEdC,EAAAA,cAAc,EAAE,wBAAC1C,KAAD,EAAO2C,UAAP;AAAA,WAAoB3C,KAAK,CAACC,WAAN,GAAkB0C,UAAtC;AAAA,GAFF;AAGdC,EAAAA,WAAW,EAAE,qBAAC5C,KAAD,EAAOE,QAAP;AAAA,WAAkBF,KAAK,CAACE,QAAN,GAAeA,QAAjC;AAAA,GAHC;AAId2C,EAAAA,WAAW,EAAE,qBAAC7C,KAAD,EAAO8C,QAAP;AAAA,WAAkB9C,KAAK,CAAC8C,QAAN,GAAeA,QAAjC;AAAA,GAJC;AAKdC,EAAAA,WAAW,EAAE,qBAAC/C,KAAD,EAAOI,QAAP;AAAA,WAAkBJ,KAAK,CAACI,QAAN,GAAeA,QAAjC;AAAA,GALC;AAMd;AACA4C,EAAAA,MAAM,EAAE,gBAAChD,KAAD,EAAOiD,MAAP;AAAA,WAAgBjD,KAAK,CAACO,GAAN,GAAU0C,MAA1B;AAAA,GAPM;AAQdC,EAAAA,gBAAgB,EAAE,0BAAClD,KAAD,EAAOiD,MAAP;AAAA,WAAgBjD,KAAK,CAACQ,aAAN,GAAoByC,MAApC;AAAA,GARJ;AASdE,EAAAA,WAAW,EAAE,qBAACnD,KAAD,EAAOiD,MAAP;AAAA,WAAgBjD,KAAK,CAACU,SAAN,GAAgBuC,MAAhC;AAAA;AATC,CAAlB;AAcJ,eAAe;AACXjD,EAAAA,KAAK,EAALA,KADW;AACLmC,EAAAA,OAAO,EAAPA,OADK;AACGrB,EAAAA,OAAO,EAAPA,OADH;AACW2B,EAAAA,SAAS,EAATA;AADX,CAAf","sourcesContent":["import axios from 'axios'\r\nimport constants from '../../plugins/constants'\r\n\r\n//const resource_url = \"http://36fd800a6734.ngrok.io/api/v1/\";\r\n\r\n//vendeurs\r\n//contacts\r\n//filtre\r\n    const state = {\r\n        catproducts:[],\r\n        products: [],\r\n        sellers: [],\r\n        contacts: [],\r\n        selectCat:1,\r\n        resultsearch:[],\r\n        box:false,\r\n        encaissements:{},\r\n        vehicules:{},\r\n        clientsId:0,\r\n        vendeursId:0,\r\n        contactsId:0,\r\n        resource_url: constants.resource_url\r\n    };\r\n\r\n    const actions = {\r\n        updateClientsId({commit},data){\r\n            commit(\"SETCLIENTID\",data.clientid)\r\n        },\r\n        async FetchCatProducts({commit}){\r\n\r\n             axios.get(state.resource_url+'catproducts')\r\n                .then(products => {\r\n                    commit(\"SETCATPRODUCTS\",products.data.data)\r\n\r\n                })\r\n                .catch(function(error) {\r\n                    console.log('catproducts_error',error);\r\n                    // Handle Errors here.\r\n                    // var errorCode = error.code;\r\n                    // var errorMessage = error.message;\r\n                    // console.log(error);\r\n\r\n                    //commit(\"setError\", error);\r\n                    // ...\r\n                });\r\n            // response = response.data\r\n            //console.log('response',response.data.data);\r\n\r\n        },\r\n\r\n        async FetchProducts({commit},data){\r\n\r\n            this.selectCat = data.categorieProduitId;\r\n             axios.get(state.resource_url+'categorieproduits/'+data.categorieProduitId+'/listesproduits')\r\n\r\n                .then(products => {\r\n                    console.log('products',products.data.data);\r\n                    commit(\"SETPRODUCTS\",products.data.data)\r\n                })\r\n\r\n                .catch(function(error) {\r\n                    console.log('products_error',error);\r\n                    // Handle Errors here.\r\n                    // var errorCode = error.code;\r\n                    // var errorMessage = error.message;\r\n                    // console.log(error);\r\n                    // commit(\"setError\", error);\r\n                    // ...\r\n\r\n                });\r\n\r\n            //console.log('response',response.data.data);\r\n\r\n        },\r\n\r\n        async searchReference({commit},data){\r\n\r\n            console.log('commit',data)\r\n            axios.get(state.resource_url+'produits/reference/'+data)\r\n\r\n                .then(products => {\r\n\r\n                    console.log('products',products.data.data);\r\n                    commit(\"SETPRODUCTS\",products.data.data)\r\n\r\n                })\r\n\r\n                .catch(function(error) {\r\n                    console.log('products_error',error);\r\n                    // Handle Errors here.\r\n                    // var errorCode = error.code;\r\n                    // var errorMessage = error.message;\r\n                    // console.log(error);\r\n                    // commit(\"setError\", error);\r\n                    // ...\r\n\r\n                });\r\n\r\n            //console.log('response',response.data.data);\r\n\r\n        },\r\n\r\n        async FetchSellers({commit}){\r\n\r\n            axios.get(state.resource_url+'vendeurs')\r\n                .then(sellers => {\r\n                    console.log('vendeurs',sellers.data.data);\r\n                    commit(\"SETVENDEURS\",sellers.data.data)\r\n                })\r\n                .catch(function(error) {\r\n                    console.log('products_error',error);\r\n                    // Handle Errors here.\r\n                    // var errorCode = error.code;\r\n                    // var errorMessage = error.message;\r\n                    // console.log(error);\r\n\r\n                    //commit(\"setError\", error);\r\n\r\n                });\r\n            //commit(\"SETPRODUCTS\",[{id: 1,image:\"\",libelle:\"Categorie A\",categorie:\"1\",reference:\"LTPOLART\",prix:11500},{id: 2,image:\"\",libelle:\"Categorie C\",categorie:\"1\",reference:\"LTJJHHUGI\",prix:9500}])\r\n            //console.log('response',response.data.data);\r\n\r\n        },\r\n\r\n        async FetchContacts({commit}){\r\n\r\n            axios.get(state.resource_url+'contacts')\r\n                .then(contacts => {\r\n                    console.log('contacts',contacts.data.data);\r\n                    commit(\"SETCONTACTS\",contacts.data.data)\r\n                })\r\n                .catch(function(error) {\r\n                    console.log('products_error',error);\r\n                    // Handle Errors here.\r\n                    // var errorCode = error.code;\r\n                    // var errorMessage = error.message;\r\n                    // console.log(error);\r\n\r\n                    //commit(\"setError\", error);\r\n\r\n                });\r\n            //commit(\"SETPRODUCTS\",[{id: 1,image:\"\",libelle:\"Categorie A\",categorie:\"1\",reference:\"LTPOLART\",prix:11500},{id: 2,image:\"\",libelle:\"Categorie C\",categorie:\"1\",reference:\"LTJJHHUGI\",prix:9500}])\r\n            //console.log('response',response.data.data);\r\n\r\n        },\r\n\r\n        inAccounts({commit},data){\r\n            axios.post(state.resource_url + 'encaissements', data)\r\n                .then(encaissements => {\r\n                    if (encaissements.data.success) {\r\n                        //objet client qui vient detre ajoute\r\n                        //commit(\"SETCUSTOMER\", customers.data.data)\r\n                        commit(\"SETBOX\", true)\r\n                        commit(\"SETENCAISSEMENTS\", encaissements.data.data)\r\n\r\n                    }\r\n                })\r\n                .catch(function (error) {\r\n                    console.log('customer_create_error', error);\r\n                    //commit(\"SETBOX\", true)\r\n                    // Handle Errors here.\r\n                    // var errorCode = error.code;\r\n                    // var errorMessage = error.message;\r\n                    // console.log(error);\r\n                    //commit(\"setError\", error);\r\n                    // ...\r\n                });\r\n\r\n        },\r\n\r\n        async inBoxs({commit},data) {\r\n\r\n            axios.post(state.resource_url + 'encaissements', data)\r\n                .then(encaissements => {\r\n                    if (encaissements.data.success) {\r\n                        //objet client qui vient detre ajoute\r\n                        //commit(\"SETCUSTOMER\", customers.data.data)\r\n                        commit(\"SETBOX\", true)\r\n                        commit(\"SETENCAISSEMENTS\", encaissements.data.data)\r\n\r\n                        console.log('ok',encaissements)\r\n                    }\r\n                })\r\n                .catch(function (error) {\r\n                    console.log('customer_create_error', error);\r\n                    //commit(\"SETBOX\", true)\r\n                    // Handle Errors here.\r\n                    // var errorCode = error.code;\r\n                    // var errorMessage = error.message;\r\n                    // console.log(error);\r\n                    //commit(\"setError\", error);\r\n                    // ...\r\n                });\r\n        }\r\n    }\r\n\r\n    const getters = {\r\n        getProducts: state => state.products,\r\n        getCatProducts: state => state.catproducts,\r\n        //GetResultSearch: state => state.resultsearch,\r\n        GetBox:state => state.box,\r\n        GetEncaissements:state => state.encaissements,\r\n        GetClientId:state => state.clientsId\r\n    }\r\n\r\n    const mutations = {\r\n\r\n        SETCATPRODUCTS: (state,catproduct)=>state.catproducts=catproduct,\r\n        SETPRODUCTS: (state,products)=>state.products=products,\r\n        SETVENDEURS: (state,vendeurs)=>state.vendeurs=vendeurs,\r\n        SETCONTACTS: (state,contacts)=>state.contacts=contacts,\r\n        //SETRESULTSEARCH: (state,result)=>state.resultsearch=result\r\n        SETBOX: (state,result)=>state.box=result,\r\n        SETENCAISSEMENTS: (state,result)=>state.encaissements=result,\r\n        SETCLIENTID: (state,result)=>state.clientsId=result\r\n\r\n    };\r\n\r\n\r\nexport default {\r\n    state,getters,actions,mutations\r\n}"]}]}