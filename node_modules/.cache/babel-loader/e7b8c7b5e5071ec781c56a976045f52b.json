m \"vuedraggable\";\r\n    export default {\r\n\r\n        display: \"ListKanban\",\r\n        props:{\r\n            rows_kanban:Array,\r\n            nbre:Number\r\n        },\r\n        mounted(){\r\n            this.colors = this.$store.state.visits.colors\r\n        },\r\n        data(){\r\n            return {\r\n                drag:true,\r\n                colors:[]\r\n            }\r\n        },\r\n        components: {\r\n            draggable\r\n        },\r\n        methods: {\r\n\r\n        },\r\n        computed: {\r\n            dragOptions() {\r\n                return {\r\n                    animation: 200,\r\n                    group: \"description\",\r\n                    disabled: false,\r\n                    ghostClass: \"ghost\"\r\n                };\r\n            },\r\n            sizes(){\r\n\r\n                return (12/this.nbre)\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n\r\n<style >\r\n\r\n    .mb-30{\r\n        margin-bottom: 10px !important;\r\n    }\r\n    .list-group-item{\r\n        margin:0;\r\n\r\n    }\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/components"}]}