w ? 'true' : null\" @submit.prevent=\"registerCatProducts(type)\">\n\n                    <b-form-group\n                            class=\"col-md-10 mb-30\"\n                            :label=\"$t('nom_label')\"\n                            label-for=\"input-1\"\n                    >\n\n                        <b-form-input\n\n                                v-model.trim=\"$v.nom.$model\"\n                                type=\"text\"\n                                :placeholder=\"$t('prooftag')\"\n                        ></b-form-input>\n                        <b-alert\n                                show\n                                variant=\"danger\"\n                                class=\"error col-md-12 mt-1\"\n                                v-if=\"!$v.nom.minLength\"\n                        >{{$t('validator_nom')}}\n                            {{ $v.nom.$params.minLength.min }} {{$t('letters')}}  .</b-alert\n                        >\n\n\n                    </b-form-group>\n\n\n                    <b-form-group\n                            class=\"col-md-12 mb-30\"\n                            :label=\"$t('desc_inf')\"\n                            label-for=\"input-1\"\n                    >\n                        <b-row>\n                            <wysiwyg v-model=\"content\" />\n                        </b-row>\n\n                    </b-form-group>\n\n\n                    <p></p>\n\n                    <div style=\"text-align: right\">\n                        <b-button  @click=\"hide()\" variant=\"outline-danger\" style=\"margin-right: 15px\">{{$t('Annuler')}}</b-button>\n                        <b-button type=\"submit\" variant=\"outline-success rights\" style=\"position: relative;right: 0;margin-right: 10px;\">{{$t('Enregistrer')}}</b-button>\n                    </div>\n\n                    <p v-once class=\"typo__p\" v-if=\"submitStatus === 'OK'\">\n                        <!--{{ makeToast(\"success\",\"success\") }}-->\n                    </p>\n                    <p v-once class=\"typo__p\" v-if=\"submitStatus === 'ERROR'\">\n                        {{ makeToast(\"Erreur\",\"error\") }}\n                    </p>\n                    <div v-once class=\"typo__p\" v-if=\"submitStatus === 'PENDING'\">\n                        <div class=\"spinner sm spinner-primary mt-3\"></div>\n                    </div>\n\n                </b-form>\n            </b-overlay>\n        </template>\n\n    </b-modal>\n\n</template>\n\n<script>\n\n    import { required, minLength } from \"vuelidate/lib/validators\";\n\n    export default {\n        name:\"formcatProducts\",\n        props:{\n            id:String,\n            title:String,\n            type:String,\n            statut:Boolean\n        },\n        data() {\n            return {\n                submitStatus: null,\n                nom: '',\n                content:'',\n                status: false,\n                show: false\n            }\n        },\n        validations: {\n            nom: {\n                required,\n                minLength: minLength(4)\n            }\n        },\n\n        methods:{\n            registerCatProducts: function(item){\n\n                let test={}\n                test = { variant:item, libelle: this.nom,\n                    description:this.content }\n\n                this.show = !this.show\n                this.$v.$touch();\n\n                if (this.$v.$invalid) {\n\n                    this.submitStatus = \"ERROR\";\n                    this.show = !this.show\n                } else {\n                    // do your submit logic here\n                    this.submitStatus = \"PENDING\";\n                    setTimeout(() => {\n                        this.submitStatus = \"OK\";\n                        this.$emit(\"registercatproducts\",test);\n                    }, 1000);\n                }\n\n                //\n            },\n\n            makeToast(variant = null,type=\"info\") {\n\n                this.$toasted.show(variant,{type:type})\n            },\n\n            makeToastTwo(variant = null) {\n                console.log('Successfully Submitted')\n\n                this.$bvToast.toast(\"Successfully Submitted\", {\n                    title: `Variant ${variant || \"default\"}`,\n                    variant: variant,\n                    solid: true\n                });\n            },\n        }\n    }\n</script>"]}]}