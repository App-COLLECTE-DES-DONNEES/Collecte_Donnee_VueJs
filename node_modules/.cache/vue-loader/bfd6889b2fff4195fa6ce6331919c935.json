ge',params)\r\n                this.currentpage = params\r\n                this.loadanotherpage = true\r\n\r\n               // this.visiteenattente(getRequestParams(this.filter,this.currentpage,this.pagesize,this.$i18n.locale))\r\n\r\n\r\n            },\r\n        },\r\n/**/\r\n        data() {\r\n\r\n            return {\r\n                totalPages_:0,\r\n                loadanotherpage:false,\r\n                totalElement:0,\r\n                links:[],\r\n                list:[],\r\n                rowe:{},\r\n                columns:[\r\n                    {\r\n                        label: \"Id \",\r\n                        field: \"id\",\r\n                       hidden: false,\r\n                    },\r\n                    {\r\n                        label: \"Identity \",\r\n                        field: \"identity\",\r\n                       hidden: false,\r\n                    },\r\n                    {\r\n                        label: \"Sexe \",\r\n                        field: \"gender\",\r\n                       hidden: false,\r\n                    },\r\n                    {\r\n                        label: \"Gravite des traumatismes \",\r\n                        field: \"personTraumaSeverity\",\r\n                       hidden: false,\r\n                    },\r\n                    {\r\n                        label: \"Place\",\r\n                        field: \"place\",\r\n                       hidden: false,\r\n                    },\r\n                    {\r\n                        label: \"Date Accident\",\r\n                        field: \"crashDate\",\r\n                       hidden: false,\r\n                    },\r\n                    {\r\n                        label: \"Date de Naissance \",\r\n                        field: \"birthDate\",\r\n                        hidden: false,\r\n                    },\r\n                    {\r\n                        label: \"Status\",\r\n                        field: \"