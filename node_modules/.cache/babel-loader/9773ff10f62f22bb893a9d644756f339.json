uccess')\r\n                    this.$router.push({ name: 'visit_list',params:{id:1} })\r\n                }else{\r\n                    this.makeToast('signature ','error')\r\n                }\r\n            }\r\n        }\r\n    };\r\n</script>"],"sourceRoot":"src/views/app/visit"}]}