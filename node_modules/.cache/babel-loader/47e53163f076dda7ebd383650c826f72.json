=\"input-1\"\r\n                >\r\n                    <b-form-input\r\n\r\n                            v-model=\"nom\"\r\n                            type=\"text\"\r\n\r\n                            :placeholder=\"$t('prooftag')\"\r\n                    ></b-form-input>\r\n\r\n                </b-form-group>\r\n                <b-form-group\r\n                        class=\"col-md-10 mb-30\"\r\n                        label=\"Prenom:*\"\r\n                        label-for=\"input-1\"\r\n                >\r\n                    <b-form-input\r\n\r\n                            v-model=\"prenom\"\r\n                            type=\"text\"\r\n                            :placeholder=\"$t('prenom')\"\r\n                    ></b-form-input>\r\n\r\n                </b-form-group>\r\n                <b-form-group\r\n                        class=\"col-md-10 mb-30\"\r\n                        :label=\"$t('tel')\"\r\n                        label-for=\"input-1\"\r\n                >\r\n                    <b-form-input\r\n\r\n                            v-model=\"phone\"\r\n                            type=\"number\"\r\n\r\n                            :placeholder=\"$t('tels')\"\r\n                    ></b-form-input>\r\n\r\n                </b-form-group>\r\n\r\n                <b-form-group\r\n                        class=\"col-md-10 mb-30\"\r\n                        :label=\"$t('birthday')\"\r\n                        label-for=\"input-1\"\r\n                >\r\n                    <b-form-datepicker id=\"example-datepicker\"  v-model=\"birthday\" class=\"mb-2\"></b-form-datepicker>\r\n\r\n                </b-form-group>\r\n                <b-form-group\r\n                        class=\"col-md-10 mb-30\"\r\n                        :label=\"$t('passports')\"\r\n                        label-for=\"input-1\"\r\n                >\r\n                    <b-form-input\r\n\r\n                            v-model=\"passport\"\r\n                            type=\"number\"\r\n                            :placeholder=\"$t('passport')\"\r\n                    ></b-form-input>\r\n\r\n                </b-form-group>\r\n                <b-form-group\r\n                        class=\"col-md-10 mb-30\"\r\n                        :label=\"$t('permi')\"\r\n                        label-for=\"input-1\"\r\n                >\r\n                    <b-form-input\r\n\r\n                            v-model=\"permi_de_conduire\"\r\n                            type=\"number\"\r\n                            :placeholder=\"$t('permis')\"\r\n                    ></b-form-input>\r\n\r\n                </b-form-group>\r\n\r\n                <p></p>\r\n\r\n                <div style=\"text-align: right\">\r\n                    <b-button  @click=\"hide()\" variant=\"outline-danger\" style=\"margin-right: 15px\">{{$t('Annuler')}}</b-button>\r\n                    <b-button type=\"submit\" variant=\"outline-success rights\" style=\"position: relative;right: 0;margin-right: 10px;\">{{$t('Enregistrer')}}</b-button>\r\n                </div>\r\n\r\n                <p v-once class=\"typo__p\" v-if=\"submitStatus === 'OK'\">\r\n                    {{ makeToastTwo(\"success\") }}\r\n                </p>\r\n                <p v-once class=\"typo__p\" v-if=\"submitStatus === 'ERROR'\">\r\n                    {{ makeToast(\"danger\") }}\r\n                </p>\r\n                <div v-once class=\"typo__p\" v-if=\"submitStatus === 'PENDING'\">\r\n                    <div class=\"spinner sm spinner-primary mt-3\"></div>\r\n                </div>\r\n\r\n            </b-form>\r\n\r\n        </template>\r\n\r\n    </b-modal>\r\n</template>\r\n\r\n<script>\r\n    //import { required, sameAs, minLength } from \"vuelidate/lib/validators\";\r\n    import { required, minLength } from \"vuelidate/lib/validators\";\r\n\r\n    export default {\r\n        name:\"registerPartners\",\r\n        props:{\r\n            id:String,\r\n            title:String,\r\n            type:String,\r\n            cnis:String,\r\n            nom: String,\r\n            prenom: String,\r\n            phone: String,\r\n            status: Boolean,\r\n            birthday:String,\r\n            passport:String,\r\n            permi_de_conduire:String,\r\n            submitStatus: null,\r\n            date: new Date().toLocaleString()\r\n        },\r\n        validations: {\r\n            cnis: {\r\n                required,\r\n                minLength: minLength(9)\r\n            },\r\n        },\r\n        methods:{\r\n            registerCustomer: function(item){\r\n\r\n                    console.log(\"submit!\");\r\n\r\n                    this.$v.$touch();\r\n                    if (this.$v.$invalid) {\r\n                        this.submitStatus = \"ERROR\";\r\n                    } else {\r\n                        // do your submit logic here\r\n                        this.submitStatus = \"PENDING\";\r\n                        setTimeout(() => {\r\n                            this.submitStatus = \"OK\";\r\n                        }, 1000);\r\n                    }\r\n\r\n\r\n                //this.$emit(\"registerpartners\",item);\r\n            },\r\n            makeToast(variant = null) {\r\n                console.log('Please fill the form correctly')\r\n                this.$bvToast.toast(\"Please fill the form correctly.\", {\r\n                    title: `Variant ${variant || \"default\"}`,\r\n                    variant: variant,\r\n                    solid: true\r\n                });\r\n            },\r\n\r\n            makeToastTwo(variant = null) {\r\n                console.log('Successfully Submitted')\r\n\r\n                this.$bvToast.toast(\"Successfully Submitted\", {\r\n                    title: `Variant ${variant || \"default\"}`,\r\n                    variant: variant,\r\n                    solid: true\r\n                });\r\n            },\r\n        }\r\n    }\r\n</script>"],"sourceRoot":"src/views/app/components"}]}